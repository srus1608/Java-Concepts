package com.cisco;

interface Vehicle{
	void milage();
	void wheels();
}

class Car implements Vehicle{
	@override
	public void wheels() {
		System.out.println("4 wheeler");
	}
}
class Nexon implements Car{
	
}

public class TestAbstraction {
	public static void print(Vehicle v) {
		v.milage();
		v.wheels();
		System.out.println("--------------------");
	}

}
